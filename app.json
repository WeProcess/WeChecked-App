{
  "expo": {
    "name": "WeChecked",
    "slug": "wechecked",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "runtimeVersion": "49.0.16",
    "platforms": ["ios", "android"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c2d31"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c930f780-77c5-4e55-9ce3-42de046de304"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.karmamgmt.iwechecked",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      },
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "android": {
      "package": "com.karmamgmt.wechecked",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "plugins": ["@react-native-firebase/app", "@react-native-firebase/auth"],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c930f780-77c5-4e55-9ce3-42de046de304"
      }
    },
    "owner": "rohitskarma"
  }
